{
  "name": "incomplete-niwa-archives",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "prebuild": "rm -rf dist",
    "build": "nuxt build",
    "start": "nuxt start",
    "pregenerate": "rm -rf dist",
    "generate": "nuxt generate",
    "rename": "mv ./dist/htaccess ./dist/.htaccess; mv ./dist/htpasswd ./dist/.htpasswd",
    "rename-back": "mv ./dist/.htaccess ./dist/htaccess; mv ./dist/.htpasswd ./dist/htpasswd",
    "deploy:test": "sh deploy.sh test",
    "deploy:production": "sh deploy.sh production",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js"
  },
  "config": {
    "nuxt": {
      "host": "192.168.0.102",
      "port": "3000"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.0.2",
    "change-case": "^4.1.2",
    "core-js": "^3.6.5",
    "node-sass": "^5.0.0",
    "nuxt": "^2.14.6",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^10.1.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2"
  }
}
